            ]
        ]
    },
    {
        "id": "73a980f372fdaf65",
        "type": "function",
        "z": "7649faa3400455b0",
        "g": "0be6b0791d82993a",
        "name": "InfluxDB Mapper",
        "func": "\nif (!msg.payload?.result?.detections) {\n    return null;\n}\n\nconst detections = msg.payload.result.detections;\nconst meta = msg.payload;\n\nmsg.measurement = \"edge_ai_detection\";\n\nmsg.tags = {\n    model: meta.model.identifier,\n    version: meta.model.version,\n    source: meta.tags.source,\n    class: \"rubber-ducky\"\n};\n\nmsg.payload = detections.map((det, index) => ({\n    confidence: Number(det.score),\n    bbox_x: det.boundingBox.x,\n    bbox_y: det.boundingBox.y,\n    bbox_width: det.boundingBox.width,\n    bbox_height: det.boundingBox.height,\n    object_count: meta.tags.objectCount,\n    object_index: index\n}));\n\nreturn msg;",
        "outputs": 1,
