    {
        "id": "73a980f372fdaf65",
        "type": "function",
        "z": "7649faa3400455b0",
        "name": "InfluxDB Mapper",
        "func": "const detections = msg.detections || [];\nconst modelName = msg.model || \"duck-detector\";\n\nif (!detections.length) return null;\n\n// Create ONE InfluxDB point per detection\nmsg.payload = detections.map(d => ({\n    measurement: \"edge_ai_detection\",\n    tags: {\n        camera: d.camera,\n        label: d.label,\n        model: modelName\n    },\n    fields: {\n        confidence: d.confidence,\n        bbox_x: d.bbox.x,\n        bbox_y: d.bbox.y,\n        bbox_width: d.bbox.width,\n        bbox_height: d.bbox.height,\n        detected: true\n    },\n    timestamp: d.timestamp\n}));\n\nreturn msg;",
