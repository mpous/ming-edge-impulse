    {
        "id": "4a3fddc5f86ef4eb",
        "type": "function",
        "z": "7649faa3400455b0",
        "name": "Edge Impulse Inference",
        "func": "try {\n    msg.payload = JSON.parse(msg.payload);\n    msg.camera = 'video2';\n    msg.model = '';\n    \n    const boxes = msg.payload?.result?.bounding_boxes || [];\n    const ts = Date.now();\n\n    if (!Array.isArray(boxes) || boxes.length === 0) {\n        return null; // No detections â†’ stop flow\n    }\n\n    msg.detections = boxes.map((b, index) => ({\n        id: `${msg._msgid}-${index}`,\n        camera: \"video2\",\n        label: b.label,\n        confidence: Number(b.value),\n        bbox: {\n            x: b.x,\n            y: b.y,\n            width: b.width,\n            height: b.height\n        },\n        timestamp: ts\n    }));\n\n    return msg;\n\n    \n} catch (err) {\n    node.error(\"Failed to parse Edge Impulse response\", msg);\n    return null;\n}",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
