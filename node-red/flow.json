    {
        "id": "f44edc915090c519",
        "type": "function",
        "z": "7649faa3400455b0",
        "g": "8c4d2cc81e36a6f3",
        "name": "MQTT Inference Mapper",
        "func": "try {\n    // ---- Parse Edge Impulse response if needed\n    if (typeof msg.payload === \"string\") {\n        msg.payload = JSON.parse(msg.payload);\n    }\n\n    const camera = \"video2\";\n    const modelId = \"duck-detector\";\n    const modelVersion = \"1.0.0\";\n    const modelName = \"Duck Detection\";\n\n    // ---- Edge Impulse detections\n    const boxes = msg.payload?.result?.bounding_boxes || [];\n\n    if (!Array.isArray(boxes) || boxes.length === 0) {\n        return null; // No detections â†’ stop flow early\n    }\n\n    // ---- Build detection objects (HiveMQ / Edge AI format)\n    const detections = boxes.map(b => ({\n        class: b.label,\n        score: Number(b.value),\n        boundingBox: {\n            x: Number(b.x),\n            y: Number(b.y),\n            width: Number(b.width),\n            height: Number(b.height)\n        }\n    }));\n\n    // ---- Final MQTT payload\n    msg.payload = {\n        identifier: `inference-${msg._msgid}`,\n        model: {\n            identifier: modelId,\n            version: modelVersion,\n            name: modelName\n        },\n        tags: {\n            source: camera,\n            imagePath: msg.filename,\n            objectCount: detections.length\n        },\n        resultType: \"detections\",\n        result: {\n            detections\n        },\n        explanation: {}\n    };\n\n    // ---- MQTT topic\n    msg.topic = `enterprise/site/area/line/cell/${camera}/${modelId}/${modelVersion}/inference`;\n\n    return msg;\n\n} catch (err) {\n    node.error(\"Failed to process Edge Impulse inference\", err);\n    return null;\n}\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 830,
        "y": 220,
        "wires": [
